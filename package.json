{
  "name": "taxonomist",
  "version": "1.0.3",
  "description": "Assign objects to categories based on an object property.",
  "author": "Lim Yuan Qing",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/yuanqing/taxonomist.git"
  },
  "dependencies": {
    "jaunt": "^1.1.3"
  },
  "devDependencies": {
    "istanbul": "^0.3.5",
    "jshint": "^2.5.11",
    "jshint-stylish": "^1.0.0",
    "minijasminenode2": "^1.0.0"
  },
  "scripts": {
    "lint": "node_modules/.bin/jshint --reporter=node_modules/jshint-stylish/stylish.js --verbose index.js test/*.spec.js",
    "test": "rm -rf coverage && node_modules/.bin/istanbul cover -- node_modules/.bin/minijasminenode2 test/*.spec.js --verbose",
    "build": "npm run lint && npm test"
  },
  "keywords": [
    "categorise",
    "category",
    "group",
    "grouping",
    "object",
    "property",
    "tag",
    "taxonomy"
  ]
}
